{
  "multipart": [
    {
      "apply": {
        "model": "tropicraft:block/mangrove_boardwalk_short",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "type": "short|short_post",
        "axis": "x"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/mangrove_boardwalk_tall",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "type": "tall|tall_post|tall_post_front|tall_post_back|tall_post_front_back",
        "axis": "x"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/mangrove_boardwalk_tall_connection",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "type": "tall_post_back|tall_post_front_back",
        "axis": "x"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/mangrove_boardwalk_tall_connection",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "type": "tall_post_front|tall_post_front_back",
        "axis": "x"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/mangrove_boardwalk_short",
        "uvlock": true
      },
      "when": {
        "type": "short|short_post",
        "axis": "z"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/mangrove_boardwalk_tall",
        "uvlock": true
      },
      "when": {
        "type": "tall|tall_post|tall_post_front|tall_post_back|tall_post_front_back",
        "axis": "z"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/mangrove_boardwalk_tall_connection",
        "uvlock": true
      },
      "when": {
        "type": "tall_post_back|tall_post_front_back",
        "axis": "z"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/mangrove_boardwalk_tall_connection",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "type": "tall_post_front|tall_post_front_back",
        "axis": "z"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/mangrove_boardwalk_short_post"
      },
      "when": {
        "type": "short_post"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/mangrove_boardwalk_tall_post"
      },
      "when": {
        "type": "tall_post|tall_post_front|tall_post_back|tall_post_front_back"
      }
    }
  ]
}