{
  "multipart": [
    {
      "apply": {
        "model": "tropicraft:block/black_mangrove_roots_stem"
      },
      "when": {
        "tall": "true"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/black_mangrove_roots_stem_short"
      },
      "when": {
        "tall": "false"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/black_mangrove_roots_appendages_ground"
      },
      "when": {
        "grounded": "true",
        "tall": "true"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/black_mangrove_roots_appendages_ground_short"
      },
      "when": {
        "grounded": "true",
        "tall": "false"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/black_mangrove_roots_appendages_high"
      },
      "when": {
        "grounded": "false",
        "tall": "true"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/black_mangrove_roots_appendages_high_short"
      },
      "when": {
        "grounded": "false",
        "tall": "false"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/black_mangrove_roots_connection_high",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "north": "high"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/black_mangrove_roots_connection_low",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "north": "low"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/black_mangrove_roots_connection_high",
        "uvlock": true
      },
      "when": {
        "east": "high"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/black_mangrove_roots_connection_low",
        "uvlock": true
      },
      "when": {
        "east": "low"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/black_mangrove_roots_connection_high",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "south": "high"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/black_mangrove_roots_connection_low",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "south": "low"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/black_mangrove_roots_connection_high",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "west": "high"
      }
    },
    {
      "apply": {
        "model": "tropicraft:block/black_mangrove_roots_connection_low",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "west": "low"
      }
    }
  ]
}